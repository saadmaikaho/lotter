{
    "version": 2,
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/savePhoneNumber/",
        "dest": "main.py"
      },
      {
        "src": "/token",
        "dest": "main.py"
      },
      {
        "src": "/tickets/",
        "dest": "main.py"
      },
      {
        "src": "/generate_ticket/",
        "dest": "main.py"
      },
      {
        "src": "/submit_ticket/:ticket_code",
        "dest": "main.py"
      },
      {
        "src": "/spin/:ticket_code",
        "dest": "main.py"
      },
      {
        "src": "/ticket_prize/:ticket_code",
        "dest": "main.py"
      },
      {
        "src": "/example-route/",
        "dest": "main.py"
      },
      {
        "handle": "filesystem"
      }
      
    ]
  }
  